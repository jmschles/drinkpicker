<h3>All Drinks</h3>

<ul class="drinks-list">
  <% @drinks.each do |drink| %>
    <li data-id="<%= drink.id %>">
      <%= drink.name %>
      <span class="delete-links">
         | <%= link_to "Edit", edit_drink_url(drink) %>
         | <a class="drink-deleter" href="#">Delete</a>
      </span>
    </li>
  <% end %>
</ul>